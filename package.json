{
  "name": "ngx-sharebuttons-project",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve ngx-sharebuttons-demo",
    "build-demo": "ng build ngx-sharebuttons-demo --configuration production",
    "build-lib": "ng build ngx-sharebuttons --configuration production",
    "postbuild-lib": "cpx 'projects/ngx-sharebuttons/themes/**/*.scss' ./dist/ngx-sharebuttons/themes",
    "test-lib": "ng test ngx-sharebuttons",
    "test-lib-headless": "ng test ngx-sharebuttons --watch=false --no-progress --browsers=ChromeHeadless",
    "link-lib": "npm link dist/ngx-sharebuttons && npm link ngx-sharebuttons",
    "publish-lib": "npm publish dist/ngx-sharebuttons",
    "dev:ssr": "ng run ngx-sharebuttons-demo:serve-ssr",
    "serve:ssr": "node dist/ngx-sharebuttons-demo/server/main.js",
    "build:ssr": "ng build && ng run ngx-sharebuttons-demo:server",
    "prerender": "ng run ngx-sharebuttons-demo:prerender"
  },
  "private": true,
